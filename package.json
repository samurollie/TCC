{
  "name": "eslint-plugin-k6-performance",
  "version": "1.0.0",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "npm run build && eslint k6-scripts/**/*.js",
    "test": "npm run build && node tests/no-heavy-init-context.test.js && node tests/require-check.test.js && node tests/require-tags.test.js",
    "test:all": "npm run build && npm run test && npm run lint",
    "prepublishOnly": "npm run build"
  },
  "author": {
    "name": "Samuel Lucas Vieira Lins Barbosa",
    "email": "slvlb@ic.ufal.br"
  },
  "license": "ISC",
  "description": "ESLint plugin for detecting performance issues in k6 test scripts",
  "keywords": [
    "eslint",
    "eslintplugin",
    "k6",
    "performance",
    "testing"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "abstract-syntax-tree": "^2.22.0",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "espree": "^10.4.0",
    "esquery": "^1.6.0",
    "estree-walker": "^3.0.3"
  },
  "type": "module",
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/eslint-visitor-keys": "^1.0.0",
    "@types/espree": "^10.1.0",
    "@types/esquery": "^1.5.4",
    "@types/node": "^24.3.0",
    "@typescript-eslint/rule-tester": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-visitor-keys": "^4.2.1",
    "mocha": "^11.7.4",
    "typescript": "^5.9.2"
  }
}
